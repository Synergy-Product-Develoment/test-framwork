<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Parent Suite" verbose="1">
  <!-- Global listeners that should run in all suites -->
  <listeners>
    <listener class-name="io.qameta.allure.testng.AllureTestNg"/>
  </listeners>

  <!-- Include a child suite specified via system property `child.suite.file`. Default should be set by the build. -->
  <suite-files>
    <suite-file path="${child.suite.file}"/>
  </suite-files>
</suite>
